ef","to","src","alt","viewBox","x","y","fill","xmlns","id","width","height","transform","d","stroke"],"sources":["C:/Users/molod/Documents/GitHub/fuchs-oil/fuchs-oil/src/components/politik_confidens.js"],"sourcesContent":["import React, {useEffect, useState} from 'react';\r\nimport '../css/politik_confidens.css'; // Импорт CSS стилей\r\nimport SvgHome from '../assetsImage/assets/house.svg';\r\nimport vk from '../assetsImage/assets/icons8-вконтакте.svg';\r\nimport tg from '../assetsImage/assets/icons8-телеграмма-app.svg';\r\nimport { Link } from 'react-router-dom';\r\nimport Simposium from '../assetsImage/assets_news/Bezymyannyy.png'\r\nimport RenolitDTA from '../assetsImage/assets_news/08.04.png'\r\nimport CompMc from '../assetsImage/assets_news/COMP_MC.png'\r\nimport Kaluga from '../assetsImage/assets_news/Конференция_в_Калуге.jpg'\r\n\r\n\r\n\r\n\r\n\r\n\r\nconst KorpPolitic = () => {\r\n\r\n\r\n    return (\r\n        <main>\r\n        <div className=\"contact-info-section2\">\r\n          <div className=\"contact-section-with-products\">\r\n            <div className=\"svg-container\">\r\n            <a href=\"\"><Link to=\"/index\"><img src={SvgHome} alt=\"\"/></Link></a>\r\n            </div>\r\n            <div className=\"svg-container1\">\r\n              <svg viewBox=\"0 0 24 24\" x=\"0\" y=\"0\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\r\n                <defs>\r\n                  <clipPath id=\"clip2_203_98\" xmlns=\"http://www.w3.org/2000/svg\">\r\n                    <rect width=\"24\" height=\"24\" fill=\"white\" transform=\"translate(-0.29290000000000305, 0)\" />\r\n                  </clipPath>\r\n                </defs>\r\n                <g id=\"::before\" data-node-id=\"203:107\" xmlns=\"http://www.w3.org/2000/svg\">\r\n                  <g id=\"next.svg fill\" data-node-id=\"203:108\" clip-path=\"url(#clip2_203_98)\">\r\n                    <g id=\"next.svg\" data-node-id=\"203:109\">\r\n                      <path id=\"Vector_2\" data-node-id=\"203:110\" d=\"M9.7071,7l5,5l-5,5\" stroke=\"#90979C\" stroke-width=\"2\" stroke-linecap=\"square\" stroke-linejoin=\"round\" />\r\n                    </g>\r\n                  </g>\r\n                </g>\r\n              </svg>\r\n            </div>\r\n            <p className=\"contacts-info-heading\"><a href=\"\"><Link to=\"/corpolitic\">Политика безопасности</Link></a></p>\r\n          </div>\r\n        </div>\r\n          <div className=\"contacts_info\">\r\n            <div className=\"contact-info-container-vk-tele\">\r\n                <h2 className=\"contact-info-heading1\">Политика безопасности</h2>\r\n              <div className=\"contact-info-display-style1\">\r\n              </div>\r\n            </div>\r\n            <div className=\"address-info-section-politic\">\r\n              <div className=\"info-box-politic\">\r\n                <ul className=\"title-heading-politic\">\r\n                    <li>\r\n                        <a href=\"\">Кодекс поведения</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"\">Антимонопольная политика</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"\">Антикоррупционная политика</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"\">Руководящие указания по экологически устойчивому ведению бизнеса</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"\">Руководящие указания по охране здоровья и безопасности</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"\">Глобальные руководящие указания по культурному разнообразию</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"\">Политика в области качества, охраны труда, охраны окружающей среды, пожарной и промышленной безопасности</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"\">Регламент процедуры обработки сообщений информаторов о нарушениях</a>\r\n                    </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n            <div className=\"info-politic-bez\">\r\n                <div>\r\n                  <p>\r\n                      В качестве работающей во всем мире группы компаний по производству смазочных материалов, в разных сферах ведения бизнеса FUCHS PETROLUB постоянно сталкивается с конкуренцией. И мы готовы к такой конкуренции без каких-либо ограничений. Для нас честная конкуренция является основой для этичного ведения бизнеса и прогресса, поскольку она предоставляет нам различные варианты и возможности для дальнейшего развития на рынке.\r\n                  </p>\r\n                </div>\r\n                <div>\r\n                  <p>\r\n                      В своих действиях все сотрудники FUCHS должны руководствоваться положениями действующего законодательства и соблюдать его требования. Все без исключения руководители и сотрудники обязаны соблюдать в своей профессиональной деятельности требования применимых законов, нормативных и общественных стандартов независимо от того, являются ли такие требования международными или национальными. Нарушение закона способно повредить репутации нашей компании, ослабить ее положение на рынке, а также нанести экономический ущерб.\r\n                  </p>\r\n                </div>\r\n                <div>\r\n                  <p>\r\n                      В 2004 году компания FUCHS PETROLUB внедрила <a href=\"\">Кодекс поведения</a>, который определяет, помимо прочих стандартов, принципы честной конкуренции, прозрачности и этичного ведения бизнеса. Функции корпоративного управления и контроля соответствия стандартам переданы напрямую Совету директоров. Система контроля соответствия стандартам FUCHS PETROLUB включает начальника отдела корпоративного контроля и соответствующий отдел, который оказывает поддержку и предоставляет рекомендации нашим сотрудникам по всему миру. Для каждого местного подразделения предназначен один специалист отдела корпоративного контроля.\r\n                  </p>\r\n                </div>\r\n                <div>\r\n                  <p>\r\n                      Переизданный в 2012 году <a href=\"\">Кодекс поведения</a> и руководящие указания, которые относятся к наиболее важным аспектам нормативно-правового соответствия и касаются, помимо прочего, вопросов противодействия коррупции и обеспечения конкуренции, являются обязательными к применению принципами работы, которые гарантируют постоянное соблюдение всеми сотрудниками требований законодательства и следование этическим нормам. Дополнением к этой инициативе служат информация и обучение, постоянный анализ и устранение нарушений стандартов и норм, «горячая линия» по вопросам нормативно-правового соответствия, постоянная отчетность, а также специально проводимый аудит, касающиеся нормативно-правового соответствия.\r\n                  </p>\r\n                </div>\r\n                <div>\r\n                  <h3>Система «FUCHS Compliance Communication»</h3>\r\n                  <p>\r\n                      Начиная с 15 сентября 2014 года, компания FUCHS PETROLUB использует веб-систему для отчетности, которая называется «FUCHS Compliance Communication». Эта система основана на стандартизованном решении от поставщика услуг Business Keeper AG и соответствует наивысшим стандартам ИТ-безопасности. Используя эту систему, пользователь может подробно описать свои наблюдения и начать диалог со специалистом отдела корпоративного контроля. При необходимости в течение всего процесса пользователь может оставаться анонимным.\r\n                  </p>\r\n                </div>\r\n                <div>\r\n                  <p>\r\n                      Пользователи могут быть уверены в том, что группа компаний FUCHS не заинтересована в документировании личных данных. Отсутствие гиперссылки на систему отчетов, внедренную в веб-сайт, делает отслеживание невозможным. Для получения доступа к системе «FUCHS Compliance Communication» скопируйте указанный ниже веб-адрес (URL) в адресную строку своего веб-браузера:\r\n                  </p>\r\n                  <p>\r\n                    <a href=\"https://www.bkms-system.net/FUCHS-Compliance-Communication\">https://www.bkms-system.net/FUCHS-Compliance-Communication</a>\r\n                </p>\r\n                </div>\r\n            </div>\r\n          </div>        \r\n      </main>\r\n\r\n    )\r\n}\r\n\r\nexport default KorpPolitic;"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAGC,SAAS,CAAEC,QAAQ,KAAO,OAAO,CAChD,MAAO,8BAA8B,CAAE;AACvC,MAAO,CAAAC,OAAO,KAAM,iCAAiC,CACrD,MAAO,CAAAC,EAAE,KAAM,4CAA4C,CAC3D,MAAO,CAAAC,EAAE,KAAM,iDAAiD,CAChE,OAASC,IAAI,KAAQ,kBAAkB,CACvC,MAAO,CAAAC,SAAS,KAAM,4CAA4C,CAClE,MAAO,CAAAC,UAAU,KAAM,sCAAsC,CAC7D,MAAO,CAAAC,MAAM,KAAM,wCAAwC,CAC3D,MAAO,CAAAC,MAAM,KAAM,qDAAqD,QAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAOxE,KAAM,CAAAC,WAAW,CAAGA,CAAA,GAAM,CAGtB,mBACID,KAAA,SAAAE,QAAA,eACAJ,IAAA,QAAKK,SAAS,CAAC,uBAAuB,CAAAD,QAAA,cACpCF,KAAA,QAAKG,SAAS,CAAC,+BAA+B,CAAAD,QAAA,eAC5CJ,IAAA,QAAKK,SAAS,CAAC,eAAe,CAAAD,QAAA,cAC9BJ,IAAA,MAAGM,IAAI,CAAC,EAAE,CAAAF,QAAA,cAACJ,IAAA,CAACN,IAAI,EAACa,EAAE,CAAC,QAAQ,CAAAH,QAAA,cAACJ,IAAA,QAAKQ,GAAG,CAAEjB,OAAQ,CAACkB,GAAG,CAAC,EAAE,CAAC,CAAC,CAAM,CAAC,CAAG,CAAC,CAC9D,CAAC,cACNT,IAAA,QAAKK,SAAS,CAAC,gBAAgB,CAAAD,QAAA,cAC7BF,KAAA,QAAKQ,OAAO,CAAC,WAAW,CAACC,CAAC,CAAC,GAAG,CAACC,CAAC,CAAC,GAAG,CAACC,IAAI,CAAC,MAAM,CAACC,KAAK,CAAC,4BAA4B,CAAAV,QAAA,eACjFJ,IAAA,SAAAI,QAAA,cACEJ,IAAA,aAAUe,EAAE,CAAC,cAAc,CAACD,KAAK,CAAC,4BAA4B,CAAAV,QAAA,cAC5DJ,IAAA,SAAMgB,KAAK,CAAC,IAAI,CAACC,MAAM,CAAC,IAAI,CAACJ,IAAI,CAAC,OAAO,CAACK,SAAS,CAAC,oCAAoC,CAAE,CAAC,CACnF,CAAC,CACP,CAAC,cACPlB,IAAA,MAAGe,EAAE,CAAC,UAAU,CAAC,eAAa,SAAS,CAACD,KAAK,CAAC,4BAA4B,CAAAV,QAAA,cACxEJ,IAAA,MAAGe,EAAE,CAAC,eAAe,CAAC,eAAa,SAAS,CAAC,YAAU,oBAAoB,CAAAX,QAAA,cACzEJ,IAAA,MAAGe,EAAE,CAAC,UAAU,CAAC,eAAa,SAAS,CAAAX,QAAA,cACrCJ